<!------login part------->
<section id="admin-log-in">
		<div class="container-flouid bg">
			<div class="row">
				<div class="col-md-4 col-sm-4 col-xs-12"></div>
				<div class="col-md-4 col-sm-4 col-xs-12">
					<form class="form-container" action="/adminlogin" method="POST">
						<div class="form-group">
						  <label for="ID">Admin Id</label>
						  <input type="text" class="form-control" id="ID" name="id" required id="ID">
						</div>
						<div class="form-group">
						  <label for="Password">Password</label>
						  <input type="password" class="form-control" id="Password" name="Password" required id="Password">
						</div>
						<div class="checkbox">
						  <label>
							<input type="checkbox"> Remember me
						  </label>
						</div>
						<button type="submit" class="btn btn-success btn-block">Submit</button>
					  </form>
				</div>
			</div>
			<div class="col-md-4 col-sm-4 col-xs-12"></div>
		</div>
</section>


</body>
