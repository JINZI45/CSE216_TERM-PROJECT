</section>

<!-----header---->
<!----main site---->
<main id="site-main" style="font-family: sans-serif;">
	<div class="container-fluid">
		<div class="box-nav d-flex justify-between">
			<div class="filter">
			<!--<a href="/patient-profile" class="=border-shadow">
				<h5 style="color: black;">Back to Profile</h5>
			</a>-->
			
		</div>
		</div>
	</div>
	
</main>



<!----main site----->
	<div class="container-fluid">
		<!--<div class="jumbotron bg">-->
		<div class="card">
		 <!-- <div class="card-header">Featured </div>-->
			  <div class="card-body">
			    <h5 class="card-title">Doctor List</h5>
					<form action="/doctorlist" method="POST">
					    <table class="table table-dark table-hover table-bordered text-center">
							  <thead>
							    <tr>
							      <th scope="col">Doctor ID</th>
							      <th scope="col">Name</th>
							      <th scope="col">Department</th>
							      <th scope="col">Qualification</th>
							      <th scope="col">Specialized Sector</th>
							    </tr>
							  </thead>
							  <tbody>
								<% for(var i = 0; i < row.length; i++) { %>
									<tr>
										<td><%= row[i][0] %></td>
										<td><%= row[i][1] %></td>
										<td><%= row[i][4] %></td>
										<td><%= row[i][1] %></td>
										<td><%= row[i][2] %></td>
								
									  </tr>
									  <% } %>
							  </tbody>
							</table>
						</form>




			  </div>
		</div>
			
		</div>
		
	</div>

<!----main site--->

</body>
</html>